{
  "name": "@koaris-platform/api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev src/server.ts",
    "start": "NODE_ENV=production node dist/src/server.js",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "migration:revert": "yarn typeorm migration:revert -d src/infra/database/datasource.ts",
    "migration:run": "yarn typeorm migration:run -d src/infra/database/datasource.ts",
    "migration:create": "yarn typeorm migration:create",
    "test": "jest --watch",
    "build": "tsc -p tsconfig.json --outDir dist --removeComments",
    "docker:start": "docker compose up -d",
    "docker:stop": "docker compose stop",
    "docker:drop": "docker compose down"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "casual": "^1.6.2",
    "gerador-validador-cpf": "^5.0.2",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
